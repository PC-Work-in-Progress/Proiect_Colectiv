<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="15112020-create_user_room" author="cris">
        <createTable tableName="user_room">
            <column name="user_id" type="varchar(250)">
                <constraints foreignKeyName="Id" references="users(id)"/>
            </column>
            <column name="room_id" type="varchar(250)">
                <constraints foreignKeyName="id" references="rooms(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>