<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="15112020-create_file_tag" author="cris">
        <createTable tableName="file_tags">
            <column name="file_id" type="varchar(250)">
                <constraints foreignKeyName="id" references="files(id)"/>
            </column>
            <column name="tag_id" type="varchar(250)">
                <constraints foreignKeyName="id" references="tags(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>