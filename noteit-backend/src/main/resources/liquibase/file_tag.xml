<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="15112020-create_file_tag" author="cris">
        <createTable tableName="FileTag">
            <column name="FileId" type="varchar(250)">
                <constraints foreignKeyName="Id" references="File(Id)"/>
            </column>
            <column name="TagId" type="varchar(250)">
                <constraints foreignKeyName="Id" references="Tag(Id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>